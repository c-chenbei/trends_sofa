    {%- assign paragraphs = styleCss | newline_to_br | split: '<br />' -%}
    {%- for paragraph in paragraphs %}{{- paragraph | strip -}}{% endfor -%}
{%- else -%}
    {{ styleCss }}
{%- endif -%}
