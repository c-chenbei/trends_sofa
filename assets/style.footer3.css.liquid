.back-to-top {
	  bottom: 60px;
    font-weight: 700;
    font-size: 14px;
    letter-spacing: 2px;
    position: fixed;
    right: 30px;
    color: #343e45;
    width: 40px;
    height: 40px;
    line-height: 38px;
    text-align: center;
    z-index: 9;
    overflow: hidden;
    padding: 0;
    border-radius: 3px;
    background-color: #fff;
    box-shadow: 0 0 3px #0002;
    border: 1px solid #ababab;
    align-items: center;
    display: flex;
    justify-content: center;
  	opacity:0;
}
.back-to-top::before {
    content: "\e873";
    font-family: Linearicons-Free;
}

.back-to-top:hover::before {
  color: #111111;
}

.back-to-top.show {
  opacity:1;
}

