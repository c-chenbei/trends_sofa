/*================ Style for layout RTL ================*/

body.rtl {
    direction: rtl;
}

.rtl .mfp-close-btn-in .mfp-close{
  right: auto;
  left: 0;
}

.rtl .js_popup_prpr_wrap_newsletter .wrap-newslette-popup .checkbox input{
  margin-right: 0;
  margin-left: 5px;
}

.rtl .header .header-top .header-logo{
  text-align: right;
  margin-right: 0;
  margin-left: 43px;
}

.rtl .header .header-control li:not(:first-child){
  margin-right: 20px;
  margin-left: 0;
}

.rtl .text_left, 
.rtl .left-content{
  text-align: right;
}

.rtl .text_right, 
.rtl .right-content{
  text-align: left;
}

.rtl .back-to-top{
  right: auto;
  left: 30px;
}

[dir="rtl"] .slick-next:before{
  content: "\f105";
}

[dir="rtl"] .slick-prev:before{
  content: "\f104";
}

[dir="rtl"] .slick-next{
  right: 25px;
  left: auto;
}

[dir="rtl"] .slick-prev{
  left: 25px;
  right: auto;
}

.rtl div[data-wrapper-slidershow] .slick-prev,{
  right: auto;
}

.rtl div[data-wrapper-slidershow] .slick-next{
  left: auto;
}

.rtl .thumb_right .details-thumb .slider-nav.slick-slider {
    right: auto;
    left: 0;
}

.rtl .thumb_right .details-thumb .cms-wrap-main-slider{
  padding-right: 0;
  padding-left: 120px;
}

.rtl .thumb_left .details-thumb .slider-nav.slick-slider {
    right: 0;
    left: auto;
}

.rtl .thumb_left .details-thumb .cms-wrap-main-slider{
  padding-left: 0;
  padding-right: 120px;
}

.rtl .pp_slpr_close{
  right: auto;
  left: 10px;
}

.rtl .popup_slpr_info{
  padding-left: 10px;
  padding-right: 0;
}

.rtl input.search_header__input:not([type=submit]):not([type=checkbox]){
