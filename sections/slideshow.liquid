{% assign id = section.id %}
{% assign style_dots = section.settings.style_dots %}
{% liquid
  assign section_blocks = section.blocks
  assign no_image = 'lifestyle-1' | placeholder_svg_tag: 'placeholder_svg_tag pt_100'
  assign index = 1
  assign arr_blocks_img = section_blocks | where: 'type', 'img'
%}

<section class="laber_slidershow_{{id }}">
  <div data-wrapper-slidershow class="{{ section.settings.layout }}">
    <div class="laberSlidershow">
      {% include 'grid-slider', settings: section.settings %}
      <div class="grid-slider vertical-center {% if section.settings.fade_zoom %}fade_zoom{% endif %}" {{ gridSlider }}>
        {% if arr_blocks_img.size > 0 %}
          {%- for block in arr_blocks_img -%}
            {% if block.type == 'img' %}
              {% assign image = block.settings.image_sl %}
              {% assign image_mb = block.settings.image_sl_mb %}
              {% assign url = block.settings.link_img %}
              {% assign open_link = block.settings.open_link %}
              <div id="laber_bock_{{ block.id }}" class="wrap_slide js_image_slide">
                <div class="content-item" {{ block.shopify_attributes }}>
                  {% assign ratio = image.aspect_ratio | default: 1.0 %}
                  {% assign pd_top = 1 | divided_by: ratio | times: 100 %}

                  {% assign ratio_mb = image_mb.aspect_ratio | default: 1.0 %}
                  {% assign pd_top_mb = 1 | divided_by: ratio_mb | times: 100 %}
                  <a href="{{ block.settings.link_img }}" target="{{ block.settings.open_link }}">
                    {% if image != blank %}
                      <div  class="lazyload laber_bg_lz pr_lazy_img img_slider_dek" data-bgset="{{ image | img_url: '1x1' }}" data-ratio="{{ratio}}" data-sizes="auto" style="padding-top: {{ pd_top }}%"></div>
                    {% else %}
                      <div  class="lazyload laber_bg_lz pr_lazy_img img_slider_dek">{{ no_image }}</div>
                    {% endif %}
                  </a>

                  <a href="{{ block.settings.link_img }}" target="{{ block.settings.open_link }}">
                    {% if image_mb != blank %}
                      <div class="lazyload laber_bg_lz pr_lazy_img img_slider_mb" data-bgset="{{ image_mb | img_url: '1x1' }}" data-ratio="{{ratio_mb}}" data-sizes="auto" style="padding-top: {{ pd_top_mb}}%;" ></div>
                      {% else %}
                        <div class="lazyload laber_bg_lz pr_lazy_img img_slider_mb">{{ no_image }}</div>
                      {% endif %}
                  </a>
                  <div class="wrap_text">
                    <div class="figcaption {{ block.settings.caption_width }}">
                      <div class="wrapper_caption flex flex-wrap h_{{ block.settings.h_position }} v_{{ block.settings.v_position }} text_{{ block.settings.txt_align }}">
                        <div
                          class="caption"
                          style="color:{{ block.settings.cl_overlay }}; opacity:{{ block.settings.cl_opacity | times: 10 }}"
                        >
                          <div class="outner-caption">
                            <div class="inner-caption {{ block.settings.ani_txt }}">
                              {% for block in section_blocks offset: index %}
                                {% assign index = index | plus: 1 %}
                                {%- case block.type -%}
                                  {% when 'subhd' %}
                                    <h4
                                      id="b_{{ block.id }}"
                                      class="slide_item_h4 {% if block.settings.hide %} d-none{% endif %}"
                                      {{ block.shopify_attributes }}
                                    >
                                      {% assign somearray = block.settings.title | split: ' ' %}
                                      {% assign pos = block.settings.custom_mark %}
                                      {% assign index_mark = 0 %}
                                      {% for item in somearray %}
                                        {% assign index_mark = index_mark | plus: 1 %}
                                        <span class="{% if block.settings.first_mark and forloop.first %}first {% endif %}{% if pos contains index_mark %}marked{% endif %}{% if block.settings.last_mark and forloop.last %}last {% endif %}">
                                          {{ item }}
                                        </span>
                                      {% endfor %}
                                    </h4>
                                  {% when 'hd' %}
                                    <h3
                                      id="b_{{ block.id }}"
                                      class="slide_item_h3 {% if block.settings.hide %} d-none{% endif %}"
                                      {{ block.shopify_attributes }}
                                    >
                                      {% assign somearray = block.settings.title | split: ' ' %}
                                      {% assign pos = block.settings.custom_mark %}
                                      {% assign index_mark = 0 %}
                                      {% for item in somearray %}
                                        {% assign index_mark = index_mark | plus: 1 %}
                                        <span class="{% if block.settings.first_mark and forloop.first %}first {% endif %}{% if pos contains index_mark %}marked{% endif %}{% if block.settings.last_mark and forloop.last %}last {% endif %}">
                                          {{ item }}
                                        </span>
                                      {% endfor %}
                                    </h3>
                                  {% when 'txt' %}
                                    <div
                                      id="b_{{ block.id }}"
                                      class="slide_item_p {% if block.settings.hide %} d-none{% endif %}"
                                      {{ block.shopify_attributes }}
                                    >
                                      {{ block.settings.title }}
                                    </div>
                                  {% when 'btn' %}
                                    {%- if block.settings.title != blank -%}
                                      <a
                                        id="b_{{ block.id }}"
                                        class="buttons"
                                        href="{{ block.settings.btn_link}}"
                                        target="{{open_link}}"
                                        {{ block.shopify_attributes }}
                                      >
                                        {{- block.settings.title }}
                                        {{ block.settings.icon_button -}}
                                      </a>
                                    {%- endif -%}
                                  {% when 'space' %}
                                    <div
                                      id="b_{{ block.id }}"
                                      class="slide_item_space"
                                      {{ block.shopify_attributes }}
                                    ></div>
                                  {% when 'box' %}
                                    <div id="b_{{ block.id }}" class="slide_item_box" {{ block.shopify_attributes }}>
                                      <div class="inner-box">
                                        <div class="outside-box">
                                          <div class="inside-box">
                                            <h2>{{ block.settings.title }}</h2>
                                            <p>{{ block.settings.content }}</p>
                                            {%- if block.settings.btn_link != blank
                                              and block.settings.button_text != blank
                                            -%}
                                              <a href="{{ block.settings.btn_link}}">{{ block.settings.button_text }}</a>
                                            {% endif %}
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                  {% else %}
                                    {%- break -%}
                                {%- endcase -%}
                              {%- endfor -%}
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {% endif %}
          {% endfor %}
        {% endif %}
      </div>
      {% if section.settings.dot_enable %}
        <div class="wrapper-dots  {{ style_dots }}">
          <div class="appendDots"></div>
        </div>
      {% endif %}
    </div>
  </div>
</section>

{% style %}
  .laber_slidershow_{{ id }}{
  		{% if section.settings.margin_top_slidershow!=blank %}margin-top:{{ section.settings.margin_top_slidershow}}px; {% endif %}
  		{% if section.settings.margin_bottom_slidershow !=blank %}margin-bottom:{{ section.settings.margin_bottom_slidershow}}px; {% endif %}
  		{% if section.settings.cl_bg %} background-color:{{ section.settings.cl_bg }} {% endif %}
  }
  @media (min-width: 1025px){
  	.laber_slidershow_{{ id }} .laberSlidershow{
  		{% if section.settings.padding_top_slidershow!=blank %}padding-top:{{ section.settings.padding_top_slidershow}}px; {% endif %}
  		{% if section.settings.padding_left_slidershow !=blank %}padding-left:{{ section.settings.padding_left_slidershow}}%; {% endif %}
  	}
  }
  {%- if section_blocks.size > 0 %}
         {%- for block in section_blocks -%}
         {%- assign id = block.id -%}
         {%- assign type = block.type -%}
             {%- case block.type -%}
  				{% when 'subhd' %}
                       .wrapper_caption #b_{{id }}.slide_item_h4{
                         text-transform:{{ block.settings.text_transform}};
                         letter-spacing:{{ block.settings.ls_text}}px;
                         {% if block.settings.lh_text >0 %}line-height:{{ block.settings.lh_text}}px; {% endif %}
                         font-size:{{ block.settings.fs_text}}px;
                         color:{{ block.settings.color}};
                         font-weight:{{ block.settings.fw_text}};
                       }
  					@media (max-width: 767px) {
                           .wrapper_caption #b_{{ block.id }}.slide_item_h4{
  						font-size:{{ block.settings.mobile_font_text}}px;
  						font-weight:{{ block.settings.mobile_fw_text}};
                           }
                         }
  				{% when 'hd' %}
  					.wrapper_caption #b_{{id }}.slide_item_h3 {
                           text-transform:{{ block.settings.text_transform}};
                           letter-spacing:{{ block.settings.ls_text}}px;
                           {% if block.settings.lh_text >0 %}line-height:{{ block.settings.lh_text}}px; {% endif %}
                           font-size:{{ block.settings.fs_text}}px;
                           color:{{ block.settings.color}};
                           font-weight:{{ block.settings.fw_text}};
  					}
  					@media (max-width: 767px) {
                           .wrapper_caption #b_{{ block.id }}.slide_item_h3{
  							font-weight:{{ block.settings.mobile_fw_text}};
  							font-size:{{ block.settings.mobile_font_text}}px;
                           }
                         }
  			  {% when 'txt' %}
  					.wrapper_caption #b_{{id }}.slide_item_p{
  						margin:0px;
  						letter-spacing:{{ block.settings.ls_text}}px;
  						{% if block.settings.lh_text >0 %}line-height:{{ block.settings.lh_text}}px; {% endif %}
  						font-size:{{ block.settings.fs_text}}px;
  						color:{{ block.settings.color}};
  						font-weight:{{ block.settings.fw_text}};
  						text-transform:{{ block.settings.text_transform}};
             }
             @media (max-width: 991px) {
               .wrapper_caption #b_{{ block.id }}.slide_item_p{
  						  font-size:{{ block.settings.ipad_font_text}}px;
  						  font-weight:{{ block.settings.ipad_fw_text}};
               }
             }
  					@media (max-width: 767px) {
               .wrapper_caption #b_{{ block.id }}.slide_item_p{
  						  font-size:{{ block.settings.mobile_font_text}}px;
  						  font-weight:{{ block.settings.mobile_fw_text}};
               }
             }
  			  {% when 'btn' %}
                         {% liquid
                           assign btn_pd = block.settings.pd | remove: ' ' | split: ','
                           assign pd_top = btn_pd[0]
                           assign pd_right = btn_pd[1]
                           assign pd_bottom = btn_pd[2]
                           assign pd_left = btn_pd[3]

                           assign btn_mg = block.settings.mg | remove: ' ' | split: ','
                           assign mg_top = btn_mg[0]
                           assign mg_right = btn_mg[1]
                           assign mg_bottom = btn_mg[2]
                           assign mg_left = btn_mg[3]
                         %}

                         .wrapper_caption #b_{{id }}.buttons{
                             margin-top:{{ mg_top }};
                             margin-right:{{ mg_right }};
                             margin-bottom:{{ mg_bottom }};
                             margin-left:{{ mg_left }};
                             padding-top:{{ pd_top }};
                             padding-right:{{ pd_right }};
                             padding-bottom:{{ pd_bottom }};
                             padding-left:{{ pd_left }};
                             background-color: {{ block.settings.cl_bg }};
                             color:{{ block.settings.cl_txt }};
                             font-size:{{ block.settings.fs_text}}px;
                             font-weight:{{ block.settings.fw_text}};
                             border-radius:{{ block.settings.br_button}};
                             text-transform:{{ block.settings.button_transform}};
                             letter-spacing:{{ block.settings.ls_text}}px;
                         }

                         .wrapper_caption #b_{{ block.id }}.buttons:hover{
                           background-color:{{ block.settings.cl_bg_hover}};
                           color:{{ block.settings.cl_txt_hover}}
                         }

                         .rtl .wrapper_caption #b_{{id }}.buttons{
                             padding-left: {{ pd_right }};
                             padding-right: {{ pd_left }};
                             margin-right:{{ mg_left }};
                             margin-left:{{ mg_right }};
                         }

  					@media (max-width: 767px) {
                           .wrapper_caption #b_{{ block.id }}.buttons{
                             margin:{{ block.settings.mobile_mg}};
                             padding:{{ block.settings.mobile_pd}};
                             font-size:{{ block.settings.mobile_font_text}}px;
                             font-weight:{{ block.settings.mobile_fw_text}};
                           }
                         }
  			  {% when 'space' %}
  				.wrapper_caption #b_{{ block.id }}.slide_item_space{
  					height:{{ block.settings.sp}}px
  				}
                     @media (max-width: 767px) {
  					.wrapper_caption #b_{{ block.id }}.slide_item_space{
  						height:{{ block.settings.mobile_sp}}px
                         }
                     }
  			{% when 'box' %}
  				.wrapper_caption #b_{{ block.id }}.slide_item_box .inside-box h2{
  					text-transform:{{ block.settings.title_transform}};
                         letter-spacing:{{ block.settings.ls_title }};
                         {% if block.settings.lh_title >0 %}line-height:{{ block.settings.lh_title }}px; {% endif %}
                         font-size:{{ block.settings.fs_title }}px;
                         color:{{ block.settings.color}};
                         font-weight:{{ block.settings.fw_title }};
                     }
  				.wrapper_caption #b_{{ block.id }}.slide_item_box .inside-box  p{
                         text-transform:{{ block.settings.contennt_transform}};
                         letter-spacing:{{ block.settings.ls_text}};
                         {% if block.settings.lh_text >0 %}line-height:{{ block.settings.lh_text}}px; {% endif %}
                         font-size:{{ block.settings.fs_text}}px;
                         color:{{ block.settings.color_content}};
                         font-weight:{{ block.settings.fw_text}};
                     }
  				.wrapper_caption #b_{{ block.id }}.slide_item_box .inside-box  a{
                         text-transform:{{ block.settings.btn_transform}};
                         letter-spacing:{{ block.settings.ls_btn}}px;
                         font-size:{{ block.settings.fs_text_btn}}px;
                         background-color:{{ block.settings.cl_bg_btn}};
                         color:{{ block.settings.cl_btn}};
  					font-weight:{{ block.settings.fw_text_btn}};
  				}
             {%endcase%}

     	{%endfor%}
     {%- endif- %}
     @media (max-width: 767px) {

     }
{% endstyle %}

{% javascript %}
  $(document).ready(function() {
    var button = $('.laberSlidershow .wrapper_caption').find('.buttons');
    if ( button ){
      button.parents('.wrap_text').addClass('has-button');
    }
  });
{% endjavascript %}

{% schema %}
{
  "name": "Slideshow",
  "max_blocks": 50,
  "class":"slideshow",
  "settings": [
  {
        "type": "header",
        "content": "---- Slider Show ----",
        "info": "Refer to the attribute values [here](https://kenwheeler.github.io/slick)"
      },
      {
          "type": "checkbox",
          "id": "use-slider",
          "label": "Use slider?",
          "default": true
      },
      {
          "type": "range",
          "id": "slides-To-Scroll",
          "label": "Slide to scroll",
          "info": "The number of items slide to scroll",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "visible",
          "label": "Columns in Visible",
          "info": "Item(s) in screen ( Size >= 1920 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "widescreen",
          "label": "Columns in Widescreen",
          "info": "Item(s) in screen ( 1366 <= Size < 1920 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "desktop",
          "label": "Columns in Desktop",
          "info":  "Item(s) in screen ( 1200 <= Size < 1480 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "laptop",
          "label": "Columns in Laptop",
          "info": "Item(s) in screen ( 992 <= Size < 1200 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "notebook",
          "label": "Columns in Notebook",
          "info": "Item(s) in screen ( 768 <= Size < 992 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "tablet",
          "label": "Columns in Tablet",
          "info": "Item(s) in screen ( 576 <= Size < 768 )",
          "min": 1,
          "max": 8,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "landscape",
          "label": "Columns in Landscape",
          "info": "Item(s) in screen ( 480 <= Size < 576 )",
          "min": 1,
          "max": 6,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "portrait",
          "label": "Columns in Portrait",
          "info": "Item(s) in screen ( 360 < Size < 480 )",
          "min": 1,
          "max": 6,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "mobile",
          "label": "Columns in Mobile",
          "info": "Item(s) in screen ( Size < = 360 )",
          "min": 1,
          "max": 4,
          "step": 1,
          "unit": "col",
          "default": 1
      },
      {
          "type": "range",
          "id": "rows",
          "label": "Rows Slider",
          "min": 1,
          "max": 3,
          "step": 1,
          "unit": "row",
          "default": 1
      },
      {
          "type": "checkbox",
          "id": "autoplay",
          "label": "Autoplay",
          "default": true
      },
      {
          "type": "text",
          "id": "speed",
          "label": "Speed",
          "default": "300",
          "info": "Autoplay Speed in milliseconds"
      },
      {
          "type": "text",
          "id": "autoplay-Speed",
          "label": "Auto Play Speed",
          "default": "3000",
          "info": "Autoplay Speed in milliseconds"
      },
      {
          "type": "checkbox",
          "id": "arrows",
          "label": "Arrows",
          "default": true
      },
      {
          "type": "checkbox",
          "id": "fade",
          "label": "Fade",
          "default": false,
          "info": "Only use for 1 column"
      },
      {
          "type": "checkbox",
          "id": "adaptive-Height",
          "label": "Adaptive Height",
          "default": false
      },
      {
          "type": "checkbox",
          "id": "dots",
          "label": "Dots",
          "default": false
      },
      {
          "type": "checkbox",
          "id": "infinite",
          "label": "Infinite",
          "default": false
      },
      {
          "type": "number",
          "id": "padding",
          "label": "Padding",
          "default": 15
      },
      {
        "type": "checkbox",
        "id": "fade_zoom",
        "label": "Image Zoom Effect",
        "default": true
      },
      {
        "type": "select",
        "id": "style_dots",
        "default": "default",
        "options": [
          {
            "value": "default",
            "label": "Default"
          },
        {
        "value": "style1",
        "label": "Style 1"
        },
        {
        "value": "style2",
        "label": "Style 2"
        },
        {
        "value": "style3",
        "label": "Style 3"
        }

        ],
        "label": "Dots position"
      },
  {
        "type": "header",
        "content": "== Design Options"
      },
  {
        "type": "select",
        "id": "layout",
        "default": "container",
        "options": [
          {
            "value": "container",
            "label": "Container"
          },
          {
            "value": "full-width",
            "label": "Full Width"
          }
        ],
        "label": "Layout"
      },
      {
        "type": "color",
        "id": "cl_bg",
        "label": "Background Color",
        "default": "#f5f5f5",
        "info": "only active when you use layout 'Container'"
      },

  {
        "type": "range",
        "id": "margin_top_slidershow",
        "label": "Margin top of this section",
        "min": 0,
        "max": 50,
        "step": 1,
        "default": 0
      },
      {
        "type": "range",
        "id": "margin_bottom_slidershow",
        "label": "Margin bottom of this section",
    "min": 0,
        "max": 50,
        "step": 1,
        "default": 0
      },
  {
        "type": "header",
        "content": "== Padding ",
    "info": "Only for home page 8'"
      },
      {
        "type": "range",
        "id": "padding_top_slidershow",
        "label": "Padding top ",
        "min": 0,
        "max": 50,
        "step": 1,
    "unit": "px",
        "default": 30
      },
      {
        "type": "range",
        "id": "padding_left_slidershow",
        "label": "Padding left",
    "min": 0,
        "max": 50,
        "step": 1,
    "unit": "%",
        "default": 24
      }
],
"blocks": [
    {
      "type": "img",
      "name": "Image slide (Parent)",
      "settings": [
        {
          "type": "image_picker",
          "id": "image_sl",
          "label": "Image",
          "info": "1920 x 1080px .jpg recommended"
        },
        {
          "type": "image_picker",
          "id": "image_sl_mb",
          "label": "Image mobile",
          "info": "720 x 300px .jpg recommended"
        },
        {
          "type": "url",
          "id": "link_img",
          "label": "Link image",
          "info": "Only applicable when there is no button."
        },
        {
          "type": "select",
          "id": "open_link",
          "options": [
            {
              "value": "_self",
              "label": "Current window (_self)"
            },
            {
              "value": "_blank",
              "label": "New window (_blank)"
            }
          ],
          "label": "Open link in",
          "default": "_blank"
        },
        {
            "type": "header",
            "content": "== Caption settings"
          },
        {
          "type": "select",
          "id": "caption_width",
          "default": "container",
          "options": [
            {
              "value": "container",
              "label": "Container"
            },
            {
              "value": "full-width",
              "label": "Full Width"
            }
          ],
          "label": "Caption Width"
        },
        {
          "type": "select",
          "id": "h_position",
          "label": "Horizontal Position",
          "default": "center",
          "options": [
            {
              "value": "left",
              "label": "Left"
            },
            {
              "value": "center",
              "label": "Center"
            },
            {
              "value": "right",
              "label": "Right"
            }
          ]
        },
    {
          "type": "select",
          "id": "v_position",
          "label": "Vertical Position",
          "default": "middle",
          "options": [
            {
            "value": "top",
            "label": "Top"
            },
            {
            "value": "middle",
            "label": "Middle"
            },
            {
            "value": "bottom",
            "label": "Bottom"
            }
          ]
        },
        {
          "type": "select",
          "id": "txt_align",
          "label": "Text alignment",
          "default": "center",
          "options": [
            {
              "value": "left",
              "label": "Left"
            },
            {
              "value": "center",
              "label": "Center"
            },
            {
              "value": "right",
              "label": "Right"
            }
          ]
        },
        {
          "type": "header",
          "content": "== Color"
        },
        {
          "type": "color",
          "id": "cl_overlay",
          "label": "Overlay",
          "default": "#111"
        },
