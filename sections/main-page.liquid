<div class="page-width page-width--narrow">
  <div class="container">
    <h1 class="main-page-title page-title h0">
      {{ page.title | escape }}
    </h1>
    <div class="rte">
      {{ page.content }}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "page",
  "tag": "section",
  "class": "spaced-section"
}
{% endschema %}
